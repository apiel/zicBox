
VIEW: shared_components HIDDEN

  COMPONENT: GridSequencer 0 403 480 240
    SHARED_COMPONENT: SharedGridSequencer
    KEYPAD_LAYOUT: default
      KEYMAP: 25 row 1
      KEYMAP: 36 col -1
      KEYMAP: 37 row -1
      KEYMAP: 38 col 1
      KEYMAP: 0 track 0
      KEYMAP: 1 track 1
      KEYMAP: 2 track 2
      KEYMAP: 3 track 3
      KEYMAP: 4 track 4
      KEYMAP: 5 track 5
      KEYMAP: 6 track 6
      KEYMAP: 7 track 7
      KEYMAP: 8 track 8
      KEYMAP: 9 track 9
      KEYMAP: 10 track 10
      KEYMAP: 11 track 11
      KEYMAP: 14 master
      KEYMAP: 15 clip 0
      KEYMAP: 16 clip 1
      KEYMAP: 17 clip 2
      KEYMAP: 18 clip 3
      KEYMAP: 19 clip 4
      KEYMAP: 27 clip 5
      KEYMAP: 28 clip 6
      KEYMAP: 29 clip 7
      KEYMAP: 30 clip 8
      KEYMAP: 31 clip 9
      KEYMAP: 27 clip 10
      KEYMAP: 39 clip 11
      KEYMAP: 40 clip 12
      KEYMAP: 41 clip 13
      KEYMAP: 42 clip 14
      KEYMAP: 43 clip 15
      KEYMAP: 20 param 0 50
      KEYMAP: 21 param 1 60
      KEYMAP: 22 param 2 90
      KEYMAP: 23 param 3 20
      KEYMAP: 32 param 4 50
      KEYMAP: 33 param 5 60
      KEYMAP: 34 param 6 90
      KEYMAP: 35 param 7 20
      KEYMAP: 44 param 8 50
      KEYMAP: 45 param 9 60
      KEYMAP: 46 param 10 20
      KEYMAP: 47 param 11 90

  $track=0
  while: $track < 12
    $track=$track + 1

    COMPONENT: Sample 0 300 480 100
      TRACK: $track
      AUDIO_PLUGIN: Sample 0
      DATA_STATE: 2 1
      SHARED_COMPONENT: Sample_track_$track

$track=0
while: $track < 12
  $track=$track + 1
  
  VIEW: TrackParams_track_$track
    INCLUDE: trackParams.view.ui
    USE_SHARED_COMPONENT: Sample_track_$track
    USE_SHARED_COMPONENT: SharedGridSequencer

  VIEW: StepParams_track_$track
    INCLUDE: stepParams.view.ui
    USE_SHARED_COMPONENT: Sample_track_$track
    USE_SHARED_COMPONENT: SharedGridSequencer


VIEW: MasterParams
  INCLUDE: masterParams.view.ui
  USE_SHARED_COMPONENT: SharedGridSequencer

# print: done ($track)
