  $track=1
  while: $track < $sampleTrackStart
    COMPONENT: Wave 0 300 480 100
      TRACK: $track
      KEYMAP: Keyboard 44 play 60
      PLUGIN: Kick23
      SHARED_COMPONENT: Wave_Kick23_track_$track
    $track=$track + 1

  $track=$sampleTrackStart
  while: $track < 13
    COMPONENT: Sample 0 300 480 100
      TRACK: $track
      AUDIO_PLUGIN: Sample 0
      DATA_STATE: 2 1
      SHARED_COMPONENT: Sample_track_$track
      KEYMAP: Keyboard 44 play 60
    $track=$track + 1


$track=1
while: $track < $sampleTrackStart
  VIEW: KickParams_track_$track_page_0
    INCLUDE: kickParams_page_0.view.ui
    USE_SHARED_COMPONENT: SharedGridSequencer

  VIEW: KickParams_track_$track_page_1
    INCLUDE: kickParams_page_1.view.ui
    USE_SHARED_COMPONENT: SharedGridSequencer

  VIEW: KickParams_track_$track_page_2
    INCLUDE: kickParams_page_2.view.ui
    USE_SHARED_COMPONENT: SharedGridSequencer

  VIEW: StepParams_track_$track
    INCLUDE: stepParams.view.ui
    USE_SHARED_COMPONENT: Wave_Kick23_track_1
    USE_SHARED_COMPONENT: SharedGridSequencer
  
  $track=$track + 1

$track=$sampleTrackStart
while: $track < 13
  VIEW: TrackParams_track_$track_page_0
    INCLUDE: trackParams_page_0.view.ui
    USE_SHARED_COMPONENT: Sample_track_$track
    USE_SHARED_COMPONENT: SharedGridSequencer

  VIEW: TrackParams_track_$track_page_1
    INCLUDE: trackParams_page_1.view.ui
    USE_SHARED_COMPONENT: Sample_track_$track
    USE_SHARED_COMPONENT: SharedGridSequencer

  VIEW: StepParams_track_$track
    INCLUDE: stepParams.view.ui
    USE_SHARED_COMPONENT: Sample_track_$track
    USE_SHARED_COMPONENT: SharedGridSequencer
  
  $track=$track + 1


VIEW: MasterParams
  INCLUDE: masterParams.view.ui
  USE_SHARED_COMPONENT: SharedGridSequencer

