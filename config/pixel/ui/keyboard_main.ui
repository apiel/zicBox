
  COMPONENT: KeyInfoBar 0 216
    SHIFT_VISIBILITY: 254 0
    ROW1: &icon::arrowUp Seq Clip Synth Track
    KEYMAP: Keyboard 'q' incGroup:-1
    KEYMAP: Keyboard 'w' setView:Sequencer($track)
    KEYMAP: Keyboard 'e' noteOn:($synth):58
    KEYMAP: Keyboard 'r' noteOn:($synth):59
    KEYMAP: Keyboard 't' shift:254:1:0
    ROW2: &icon::arrowDown &icon::musicNote::pixelated Live &icon::pause &icon::play
    KEYMAP: Keyboard 'a' incGroup:+1
    KEYMAP: Keyboard 's' noteOn:($synth):60
    KEYMAP: Keyboard 'd' noteOn:($synth):61
    KEYMAP: Keyboard 'f' noteOn:($synth):62
    KEYMAP: Keyboard 'g' playPause

  COMPONENT: KeyInfoBar 0 216
    SHIFT_VISIBILITY: 254 1
    ROW1: 1 2 3 4 Track
    if: $track != 1
      KEYMAP: Keyboard 'q' setView:Track1
    if: $track == 1
      KEYMAP: Keyboard 'q' setView:Sequencer1
    if: $track != 2
      KEYMAP: Keyboard 'w' setView:Track2
    if: $track == 2
      KEYMAP: Keyboard 'w' setView:Sequencer2
    if: $track != 3
      KEYMAP: Keyboard 'e' setView:Track3
    if: $track == 3
      KEYMAP: Keyboard 'e' setView:Sequencer3
    if: $track != 4
      KEYMAP: Keyboard 'r' setView:Track4
    if: $track == 4
      KEYMAP: Keyboard 'r' setView:Sequencer4
    KEYMAP: Keyboard 't' shift:254:1:0
    ROW2: 5 6 7 8 Master
    if: $track != 5
      KEYMAP: Keyboard 'a' setView:Track5
    if: $track == 5
      KEYMAP: Keyboard 'a' setView:Sequencer5
    if: $track != 6
      KEYMAP: Keyboard 's' setView:Track6
    if: $track == 6
      KEYMAP: Keyboard 's' setView:Sequencer6
    if: $track != 7
      KEYMAP: Keyboard 'd' setView:Track7
    if: $track == 7
      KEYMAP: Keyboard 'd' setView:Sequencer7
    if: $track != 8
      KEYMAP: Keyboard 'f' setView:Track8
    if: $track == 8
      KEYMAP: Keyboard 'f' setView:Sequencer8
    KEYMAP: Keyboard 'g' setView:Master
